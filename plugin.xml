      </command>
      <command
            categoryId="io.usethesource.impulse.runtime.source"
            description="Comments/uncomments the current selection or the current source line"
            id="io.usethesource.impulse.runtime.editor.toggleComment"
            name="Toggle Comment"/>
      <command
            categoryId="io.usethesource.impulse.runtime.source"
            description="Format the source code"
            id="io.usethesource.impulse.runtime.editor.formatSource"
            name="Format"/>

      <!-- navigate -->
      <command
            name="Go to Matching Fence"
            description="Moves the cursor to the matching fence character, if any"
            categoryId="org.eclipse.ui.category.navigate"
            id="io.usethesource.impulse.runtime.editor.gotoMatchingFence">
      </command>
      <command
            name="Go to Previous Navigation Target"
            description="Go to Previous Navigation Target"
            categoryId="io.usethesource.impulse.runtime.source"
            id="io.usethesource.impulse.runtime.editor.gotoPreviousTarget"/>
      <command
            name="Go to Next Navigation Target"
            description="Go to Next Navigation Target"
            categoryId="io.usethesource.impulse.runtime.source"
            id="io.usethesource.impulse.runtime.editor.gotoNextTarget"/>
      <command
            name="Open Declaration"
            description="Open an editor on the selected element"
            categoryId="org.eclipse.ui.category.navigate"
            id="io.usethesource.impulse.runtime.editor.openDeclaration">
      </command>

      <!-- -->
      <command
            categoryId="io.usethesource.impulse.runtime.source"
            description="Re-indent the selected text according to current formatting settings"
            id="io.usethesource.impulse.runtime.editor.correctIndentation"
            name="Correct Indentation">
      </command>
      <command
            categoryId="io.usethesource.impulse.runtime.source"
            description="Select the innermost enclosing construct"
            id="io.usethesource.impulse.runtime.editor.selectEnclosing"
            name="Select Enclosing">
      </command>
   </extension>
