   <extension
         point="org.eclipse.ui.contexts">
         <context
            name="IMP Source Editor"
            description="Scope for all IMP source editors"
            parentId="org.eclipse.ui.textEditorScope"
            id="io.usethesource.impulse.runtime.sourceEditorScope">
         </context>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <!--  win32:  M1=CTRL,    M2=SHIFT, M3=ALT, M4=-
            carbon: M1=COMMAND, M2=SHIFT, M3=ALT, M4=CTRL -->
      <key
            commandId="io.usethesource.impulse.runtime.editor.showOutline"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+O"/>
      <key
            commandId="io.usethesource.impulse.runtime.editor.toggleComment"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+/"/>
      <key
            commandId="io.usethesource.impulse.runtime.editor.correctIndentation"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+I"/>
      <key
            commandId="io.usethesource.impulse.runtime.editor.formatSource"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+F"/>
      <key
            sequence="M1+M3+P"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            commandId="io.usethesource.impulse.runtime.editor.gotoMatchingFence"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            sequence="M1+M3+T"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            commandId="io.usethesource.impulse.runtime.editor.refactor.quickMenu"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            sequence="M2+M3+ARROW_UP"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            commandId="io.usethesource.impulse.runtime.editor.gotoPreviousTarget"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            platform="carbon"
            sequence="M3+M4+ARROW_UP"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            commandId="io.usethesource.impulse.runtime.editor.gotoPreviousTarget"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            sequence="M2+M3+ARROW_DOWN"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            commandId="io.usethesource.impulse.runtime.editor.gotoNextTarget"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            platform="carbon"
            sequence="M3+M4+ARROW_DOWN"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            commandId="io.usethesource.impulse.runtime.editor.gotoNextTarget"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            sequence="CTRL+SHIFT+ARROW_UP"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            commandId="io.usethesource.impulse.runtime.editor.selectEnclosing"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            sequence="F3"
            contextId="io.usethesource.impulse.runtime.sourceEditorScope"
            commandId="io.usethesource.impulse.runtime.editor.openDeclaration"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="io.usethesource.impulse.runtime.source"
            name="Source"/>
      <category
            id="io.usethesource.impulse.runtime.refactor"
