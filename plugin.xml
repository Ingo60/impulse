            textStylePreferenceKey="occurrenceAnnotationTextStyleKey"
            textStylePreferenceValue="UNDERLINE"
            verticalRulerPreferenceKey="occurrenceVerticalRulerPreference"
            verticalRulerPreferenceValue="true"/>
   </extension>
